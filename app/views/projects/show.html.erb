<div class="page-header">
  <%= link_to projects_path, class: 'btn btn-primary' do %>
    All Projects
  <% end %>
  <h1><%= @project.title %></h1>
  <%= link_to edit_project_path(@project), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <%= link_to project_path(@project), method: :delete,  class: 'btn btn-primary', data: { confirm: "Are you sure? "} do %>
    Destroy
  <% end %>
</div>

